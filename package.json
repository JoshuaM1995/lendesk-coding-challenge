    "service:start": "docker-compose up -d",
    "services:start": "pnpm service:start redis",
    "service:stop": "docker-compose stop",
    "services:stop": "pnpm service:stop redis",
    "service:rm": "docker-compose rm",
    "services:rm": "pnpm service:rm redis"
